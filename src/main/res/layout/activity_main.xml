        <ImageView
            android:layout_width="4dp"
            android:layout_height="18dp"
            android:layout_marginLeft="23dp"
            android:scaleType="fitXY"
            android:src="@mipmap/technology" />

        <TextView
            android:layout_width="wrap_content"
